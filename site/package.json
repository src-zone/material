{
  "name": "material-demo",
  "version": "0.8.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist build compiled",
    "watch": "webpack --watch --progress --profile",
    "prerender": "ts-node --project tools/prerender tools/prerender/index.ts",
    "build-demo": "webpack --progress --profile --bail",
    "build": "npm run clean && npm run build-demo && npm run prerender",
    "server": "webpack-dev-server --inline --progress --port 8090",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "npm run server",
    "preinstall": "cd ../bundle && npm install && cd ../site",
    "postinstall": "npm run webdriver-update"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
