{
  "name": "material-demo",
  "version": "0.17.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf dist build compiled",
    "watch": "webpack --watch --progress --profile",
    "build-demo": "webpack --progress --profile --bail",
    "build-demo-ci": "webpack --profile --bail",
    "build": "npm run clean && npm run build-demo && prerender",
    "build-ci": "npm run clean && npm run build-demo-ci && prerender",
    "server": "webpack-dev-server --inline --progress --port 8090",
    "lint": "tslint --force \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "start": "npm run server",
    "IVYSUPPORT_COMMENT": "echo See ivysupport.md",
    "IVYSUPPORT_CLEAN": "rimraf node_modules/@blox/material && mkdirp node_modules/@blox/material",
    "IVYSUPPORT_COPY": "cpy \"dist/**/*\" ../site/node_modules/@blox/material --cwd=../bundle --parents && cpy ../bundle/package.json node_modules/@blox/material/",
    "IVYSUPPORT_APIDOCS": "cpy \"apidocs/**/*\" ../site/node_modules/@blox/material --cwd=../bundle --parents",
    "IVYSUPPORT_NGCC": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --loglevel debug",
    "IVYSUPPORT_BUILD": "cd ../bundle && npm run build && cd ../site",
    "preinstall": "cd ../bundle && npm install && cd ../site && npm run IVYSUPPORT_BUILD",
    "postinstall": "npm run IVYSUPPORT_CLEAN && npm run IVYSUPPORT_COPY && npm run IVYSUPPORT_APIDOCS && npm run IVYSUPPORT_NGCC"
  },
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/router": "^9.0.7",
    "@stackblitz/sdk": "^1.3.0",
    "angulartics2": "^8.3.0",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "classlist.js": "^1.1.20150312",
    "focus-trap": "^5.1.0",
    "material-components-web": "^0.37.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.5.4",
    "source-map-loader": "^0.2.4",
    "to-string-loader": "^1.1.6",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.900.7",
    "@angular/compiler-cli": "^9.0.7",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@blox/utils": "^0.7.0",
