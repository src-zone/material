{
  "name": "material-demo",
  "version": "0.17.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf dist build compiled",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod && prerender",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "IVYSUPPORT_COMMENT": "echo See ivysupport.md",
    "IVYSUPPORT_CLEAN": "rimraf node_modules/@blox/material && mkdirp node_modules/@blox/material",
    "IVYSUPPORT_COPY": "cpy \"dist/**/*\" ../site/node_modules/@blox/material --cwd=../bundle --parents && cpy ../bundle/package.json node_modules/@blox/material/",
    "IVYSUPPORT_APIDOCS": "cpy \"apidocs/**/*\" ../site/node_modules/@blox/material --cwd=../bundle --parents",
    "IVYSUPPORT_NGCC": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --loglevel debug",
    "IVYSUPPORT_BUILD": "cd ../bundle && npm run build && cd ../site",
    "preinstall": "cd ../bundle && npm install && cd ../site && npm run IVYSUPPORT_BUILD",
    "postinstall": "npm run IVYSUPPORT_CLEAN && npm run IVYSUPPORT_COPY && npm run IVYSUPPORT_APIDOCS"
  },
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@stackblitz/sdk": "^1.3.0",
    "angulartics2": "^8.3.0",
    "bourbon": "^7.0.0",
    "classlist.js": "^1.1.20150312",
    "focus-trap": "^5.1.0",
    "highlight.js": "^9.18.1",
    "material-components-web": "^0.37.0",
    "ng-inline-svg": "^10.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.5.4",
    "source-map-loader": "^0.2.4",
    "to-string-loader": "^1.1.6",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular-devkit/build-optimizer": "^0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "~9.1.0",
    "@blox/utils": "^0.7.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.9.5",
    "bourbon-neat": "^4.0.0",
    "codelyzer": "^5.2.2",
    "cpy-cli": "^3.1.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "mkdirp": "^1.0.3",
    "mz": "^2.7.0",
    "protractor": "~5.4.3",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "source-map-explorer": "^2.4.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
